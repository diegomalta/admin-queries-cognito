    "url": "https://github.com/diegomalta/admin-queries-cognito/issues"
  },
  "homepage": "https://github.com/diegomalta/admin-queries-cognito#readme",
  "dependencies": {
    "aws-lambda-router": "^0.9.1",
    "aws-sdk": "^2.979.0",
    "jwt-simple": "^0.5.6"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/node": "^16.11.11",
    "typescript": "^4.5.2"
  }
}
